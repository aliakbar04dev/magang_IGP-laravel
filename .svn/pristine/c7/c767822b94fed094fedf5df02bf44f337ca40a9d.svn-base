                          );


  DB::commit();
        return response()->json(['msg' => $msg]);
            } catch (Exception $ex) {
                DB::rollback();
                $msg = "Gagal submit! Hubungi Admin.";
                return response()->json(['msg' => $msg]);
            }
        }
	
	}
	 
