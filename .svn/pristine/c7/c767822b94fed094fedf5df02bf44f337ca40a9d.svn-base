			->select(DB::raw("npk"))
			->where('npk', 'like', $callkar ->first()->npk_atasan)->get();

        DB::connection("pgsql-mobile")
			->table("lupa_p_pengajuan")
			->insert([
                            'no_lp'=>$idbaru,
                            'tglpengajuan'=>$now,
                            'npk'=>Auth::User()->username,
                            'npkatasan'=>$request->npkatasan,
                            'tgllupa'=>$request->tgllupa,
