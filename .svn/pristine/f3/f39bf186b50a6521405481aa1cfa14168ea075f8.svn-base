    var url = '{{ route('datatables.popupSsr') }}';
    var lookupSsr = $('#lookupSsr').DataTable({
      processing: true,
      serverSide: true,
      "pagingType": "numbers",
      ajax: url,
      "aLengthMenu": [[5, 10, 25, 50, 75, 100, -1], [5, 10, 25, 50, 75, 100, "All"]],
      responsive: true,
      "order": [[0, 'desc'], [1, 'asc'], [3, 'asc']],
